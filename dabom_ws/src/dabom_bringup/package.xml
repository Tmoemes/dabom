<package format="3">
  <name>dabom_bringup</name>
  <version>0.0.1</version>
  <description>Master launch package for bringing up the dabom system</description>
  <maintainer email="christopher@karg.ca">Christopher Karg</maintainer>
  <license>Apache-2.0</license>

  <!-- Build tool dependencies -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_python</buildtool_depend>

  <!-- Runtime dependencies -->
  <depend>rclpy</depend>
  <depend>inv_kin</depend>
  <depend>odom</depend>
  <depend>rplidar_ros</depend>
  <depend>serial_comm</depend>
  <depend>x_serial</depend>
  <depend>nav2_bringup</depend>
  <depend>slam_toolbox</depend>
  <depend>rviz2</depend>
  <depend>joy</depend>

  <!-- Test dependencies -->
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  <test_depend>pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
